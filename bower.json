{
  "name": "D_and_angular",
  "description": "Simple todo list",
  "main": "todoApp/main.js",
  "authors": [
    "Alexander Holovin"
  ],
  "license": "MIT",
  "homepage": "",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "^1.5.8",
    "bootstrap": "^3.3.7",
    "lodash": "^4.16.6"
  },
  "overrides": {
    "angular": {
      "main": {
        "development": [
          "./angular.js"
        ],
        "production": [
          "./angular.min.js"
        ]
      }
    },
    "bootstrap": {
      "main": {
        "development": [
          "./dist/js/bootstrap.js",
          "./dist/css/bootstrap.css"
        ],
        "production": [
          "./dist/js/bootstrap.min.js",
          "./dist/css/bootstrap.min.css"
        ]
      }
    },
    "jquery": {
      "main": {
        "development": [
          "./dist/jquery.js"
        ],
        "production": [
          "./dist/jquery.min.js"
        ]
      }
    }
  }
}
